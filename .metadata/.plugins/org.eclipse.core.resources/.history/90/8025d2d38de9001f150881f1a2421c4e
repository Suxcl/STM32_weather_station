
#include <stdio.h>
#include <string.h>

#include "main.h"

typedef struct Readings{
	int s1_temp[1024];
	int s1_humi[1024];
	int s1_pres[1024];
	int s2_temp[1024];
	int s2_humi[1024];
	int s2_pres[1024];
	int s3_temp[1024];
	int s3_humi[1024];
	int s3_pres[1024];

	int s1_size;
	int s2_size;
	int s3_size;

	int size;

} Readings;

Readings newR(void);
void append(Readings r, int sensor, int t, int h, int p);
